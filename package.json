{
	"name": "csgo-dof-screenshot",
	"version": "0.1.7",
	"description": "This is a tool that helps to create screenshot with deep of field effect in Counter Strike Global Offensive.",
	"main": "electron-main/main.js",
	"scripts": {
		"build-1": "cd electron-main && webpack",
		"build-0": "npm run build-1 && cd electron-renderer && webpack",
		"build": "npm run build-0 && electron-builder",
		"build-test": "cd test && webpack",
		"test": "node test"
	},
	"build": {
		"win": {
			"target": "portable",
			"artifactName": "${name}.exe"
		},
		"linux": {
			"target": "AppImage",
			"artifactName": "${name}.appImage"
		},
		"extraResources": [
			{
				"from": "./extraResources",
				"to": ".",
				"filter": [
					"**/*"
				]
			}
		]
	},
	"keywords": [
		"csgo",
		"dof",
		"screenshot"
	],
	"author": "Thomas Riffard",
	"license": "MIT",
	"dependencies": {
		"@types/node": "^14.0.5",
		"css-loader": "^3.5.3",
		"file-loader": "^6.0.0",
		"html-webpack-plugin": "^4.3.0",
		"lodash": "^4.17.15",
		"raw-loader": "^4.0.1",
		"selenium-webdriver": "^4.0.0-alpha.7",
		"spectron": "^11.0.0",
		"style-loader": "^1.2.1",
		"temp": "^0.9.1",
		"url-loader": "^4.1.0",
		"uuid": "^8.1.0",
		"vue": "^2.6.11",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-node-externals": "^1.7.2"
	},
	"devDependencies": {
		"electron": "^11.5.0",
		"electron-builder": "^22.7.0",
		"jimp": "^0.12.1",
		"ts-loader": "^7.0.5",
		"typescript": "^3.9.3"
	}
}
